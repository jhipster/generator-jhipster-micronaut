{
  "name": "generator-jhipster-micronaut",
  "version": "0.1.0",
  "description": "A Micronaut application generator for JHipster",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-6",
    "Micronaut"
  ],
  "homepage": "https://github.com/jhipster/generator-jhipster-micronaut",
  "author": {
    "name": "Sendil Kumar N",
    "email": "sendilkumarn@live.com",
    "url": "https://sendilkumarn.com"
  },
  "bin": {
    "mhipster": "./cli/mhipster.js"
  },
  "files": [
    "generators"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhipster/generator-jhipster-micronaut.git"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "cheerio": "1.0.0-rc.2",
    "commander": "2.19.0",
    "didyoumean": "1.2.1",
    "ejs": "2.6.1",
    "faker": "4.1.0",
    "generator-jhipster": "6.8.0",
    "glob": "7.1.3",
    "insight": "0.10.1",
    "js-yaml": "3.13.1",
    "lodash": "4.17.13",
    "meow": "5.0.0",
    "mkdirp": "0.5.1",
    "pluralize": "7.0.0",
    "prettier": "1.19.1",
    "randexp": "0.5.3",
    "semver": "5.6.0",
    "shelljs": "0.8.3",
    "tabtab": "2.2.2",
    "yeoman-environment": "2.8.0",
    "yeoman-generator": "4.6.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "ejs-lint": "0.3.0",
    "eslint": "6.2.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "fs-extra": "7.0.1",
    "mocha": "6.1.4",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.2.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 20000 --slow 0 --reporter spec",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release"
  },
  "resolutions": {
    "inquirer": "6.3.1"
  },
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jhipster/generator-jhipster-micronaut/issues"
  }
}
