import { Pipe, PipeTransform } from '@angular/core';

@Pipe({ name: 'keyStartsWith' })
export class KeyStartsWith implements PipeTransform {
  transform(input: any[], filter: any, field?: string): any {
    return input.filter(entry => {
      return entry.key.indexOf(filter.toString().toLocaleLowerCase()) > -1;
    });
  }
}
